{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
{{ if isset .Params "images" }}
      {{- $.Scratch.Add "index" (dict "title" .Title "description" .Params.description "link" .Params.link "permalink" .Permalink "images" .Params.images "featured_image" .Params.featured_image) -}}
    {{ end }}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}